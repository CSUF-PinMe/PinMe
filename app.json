{
  "expo": {
    "name": "PinMe",
    "description": "Map-based social media mobile app made with React Native and AWS.",
    "slug": "PinMe",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "2.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#03a9f4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.pinme.core",
      "supportsTablet": true,
    },
    "android": {
      "package": "com.pinme.core",
      "googleServicesFile": "./google-services.json",
      }
  }
}
